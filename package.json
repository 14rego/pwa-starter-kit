{
  "name": "pwa-starter-kit",
  "version": "0.8.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "bootstrap": "npm run tools:install && ./tools/node_modules/.bin/yarn install --flat",
    "start": "./tools/node_modules/.bin/polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "./tools/node_modules/.bin/polymer build --auto-base-path && rm -rf server/build && mv build server/ && ./tools/node_modules/.bin/renamer --find node_modules --replace node_assets 'server/build/**' && ./tools/node_modules/.bin/replace node_modules node_assets server/build -r -s",
    "build:static": "./tools/node_modules/.bin/polymer build",
    "serve:prpl-server": "./tools/node_modules/.bin/prpl-server --root server/build",
    "serve:static": "./tools/node_modules/.bin/polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "./tools/node_modules/.bin/mocha test/integration --timeout=10000",
    "test:unit": "./tools/node_modules/.bin/polymer test --module-resolution=node --npm",
    "test:sauce": "./tools/node_modules/.bin/polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "./tools/node_modules/.bin/mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "tools:install": "cd tools && npm install"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.19",
    "@polymer/lit-element": "^0.5.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "pwa-helpers": "^0.8.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "wct-browser-legacy": "^1.0.1"
  },
  "resolutions": {
    "type-detect": "1.0.0",
    "supports-color": "3.1.2",
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "lodash": "3.10.1"
  }
}
